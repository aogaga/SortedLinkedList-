{
    "name": "aogaga/linked-list",
    "description": "Implementation of a sorted sorted LinkedList",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Aogaga\\LinkedList\\": "src/"
        }
    },"autoload-dev": {
    "psr-4": {
        "Aogaga\\LinkedList\\Tests\\": "tests/"
    }
},
    "authors": [
        {
            "name": "Ogheneogaga Agi",
            "email": "aogaga@yahoo.com"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.3",
        "phpstan/phpstan": "^2.1.8",
        "squizlabs/php_codesniffer": "^3.7.2"
    },
    "scripts": {
        "test": "vendor/bin/phpunit tests/ --colors=always",
        "analyse": "vendor/bin/phpstan analyse -l max",
        "fix-style": "vendor/bin/phpcbf --standard=PSR12 src tests",
        "check-style": "vendor/bin/phpcs --standard=PSR12 src tests"
    }
}
